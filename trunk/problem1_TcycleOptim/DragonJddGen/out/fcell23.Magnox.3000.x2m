          1.2000E-01 8.0000E-02 5.0000E-02 2.0000E-02 
          1.0000E-02
  MICR ALL
  SAVE ON 'FCCg' ;

DATABASE := COMPO: :: 
     STEP UP 'BOL InFuel' 
     COMM  'generic' ENDC
     PARA 'TempFuel' VALU REAL
     PARA 'TempSleeve' VALU REAL
     INIT 
     STEP UP 'BOL ExFuel' 
     COMM  'generic' ENDC
     PARA 'TempFuel' VALU REAL
     PARA 'TempSleeve' VALU REAL
     INIT 
;
DBC   := DATABASE ;

DBC := COMPO: DBC EDC :: EDIT 5 
     STEP UP 'BOL InFuel'
     TempFuel 1000.0
     TempSleeve 1000.0
     ;
DBC := COMPO: DBC EDC :: EDIT 5 
     STEP UP 'BOL ExFuel'
     TempFuel 1000.0
     TempSleeve 1000.0
     ;

FC23.Magnox.3000.DB.db   := DBC ;

