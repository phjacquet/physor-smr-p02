GCELL := GEO: ::  HOMOGE
     MIX 1
 ;
 
TRACK := SYBILT: GCELL ::
  TITLE 'Fuel'
  EDIT 1 MAXR 30 ;

*LIBMIXS := USS: LIBMIX TRACK :: EDIT 1  ;
LIBMIXS := SHI: LIBMIX TRACK :: EDIT 1 NOLJ ;


SYS := ASM: LIBMIXS TRACK ;

FLUX := FLU: SYS LIBMIXS TRACK  ::
     TYPE B B1 PNL
     EXTE 100 1E-4 THER 5 1E-4 ;

EDC := EDI: LIBMIXS TRACK FLUX ::
  EDIT 0
  MERG COMP COND 7.4080E06 3.6790E06 6.7210E05 1.1110E05 1.9310E04 
          3.3550E03 1.5850E03 7.4850E02 2.7540E02 1.3010E02 
